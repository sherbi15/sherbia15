using System;
namespace Typecasting 
{
    class Programming
    {
        static void Main(string[] args)
        {
            int a=10;
            int b=20;
            if(a>20)
            {
                Console.WriteLine("a is greater than b");
            }
            else if(a==b)
            {
                Console.WriteLine("a is equal to b");  
            }
            else
            {
                Console.WriteLine("a is not equal to b");
            }
        }
    }
    
}
// a is not equal to b
